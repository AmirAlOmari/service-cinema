{
  "name": "service-wms",
  "version": "1.17.0",
  "description": "Strategical warehouse management",
  "license": "ISC",
  "scripts": {
    "serve": "node dist/main",
    "serve:watch": "nodemon --ext ts --exec 'yarn build --incremental && yarn serve'",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "yarn generate-ajv",
    "build:watch": "tsc -w",
    "test": "tsc --noEmit && jest --runInBand --forceExit",
    "test:watch": "jest --runInBand --watch",
    "test:cover": "jest --runInBand --coverage --forceExit",
    "test:tdd": "jest --watch --runInBand --testTimeout 99999999 --onlyChanged --no-cache",
    "lint": "node -e 'require(\"./scripts/bounded-contexts-isolation/index.js\").lint()' && eslint . --ext .ts",
    "format": "prettier --write '**/*.ts'"
  },
  "dependencies": {
    "@devcubyn/carotte-runtime": "^9.0.0",
    "@devcubyn/core.errors": "^0.8.2",
    "@devcubyn/core.logger": "^1.0.1",
    "@lukeed/uuid": "^2.0.0",
    "carotte-amqp": "^3.1.0",
    "cuid": "^2.1.8",
    "debug": "^4.3.4",
    "dotenv": "^15.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.0.0",
    "moment": "^2.29.3",
    "trust-env": "^2.0.0"
  },
  "devDependencies": {
    "@devcubyn/ajv-generator": "^1.0.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "del-cli": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-cubyn-typescript": "^1.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "glob": "^7.1.7",
    "jest": "^27.0.0",
    "jest-junit": "^13.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.0.0"
  },
  "_moduleAliases": {
    "@env": "dist/env",
    "@core": "dist/core",
    "@cinema": "dist/modules/cinema"
  }
}
